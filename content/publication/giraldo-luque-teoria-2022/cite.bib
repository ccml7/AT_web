 shorttitle = {Una teoria sobre el papel de las mujeres víctimas},
 title = {Una teoria sobre el papel de las mujeres víctimas: supervivientes en la paz para Colombia.},
 url = {https://repositorio.cun.edu.co/bitstream/handle/cun/4739/Libro%20-%20Mujeres%20Victimas.pdf?sequence=1},
